<ehcache name="es">

    <diskStore path="java.io.tmpdir"/>
    
    <!-- 登录记录缓存 锁定100分钟 -->
    <cache name="iplocal"
           maxEntriesLocalHeap="20000"
           eternal="false"
           timeToIdleSeconds="36000"
           timeToLiveSeconds="0"
           overflowToDisk="false"
           statistics="false">
    </cache>

</ehcache>